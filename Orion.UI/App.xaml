            <Style TargetType="Button" BasedOn="{StaticResource MahApps.Styles.Button}" x:Key="buttonInfoStyle">

                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Grid>
                                <icon:PackIconFontAwesome Kind="InfoSolid" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <icon:PackIconFontAwesome.LayoutTransform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleX="0.75" ScaleY="0.75"/>
                                        </TransformGroup>
                                    </icon:PackIconFontAwesome.LayoutTransform>
                                </icon:PackIconFontAwesome>
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <!--<Setter Property="Background" Value="Transparent"/>-->
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding HasTitles,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="True">
                                                <Setter Property="Background" Value="#32B974"/>
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HasTitles,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="False">
                                                <Setter Property="Background" Value="Transparent"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="Command" Value="{Binding Path=DataContext.ShowItemTitlesCommand, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}"/>
                <Setter Property="CommandParameter" Value="{Binding}"/>
                <Setter Property="ToolTip" Value="Item Info"/>

                <Style.Triggers>
                    <DataTrigger Binding="{Binding HasTitles,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="False">
                        <Setter Property="Background" Value="Transparent"/>
                        <Setter Property="ToolTip" Value="Item Info"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding HasTitles,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" Value="True">
                        <Setter Property="Background" Value="#32B974"/>
                        <Setter Property="ToolTip">
                            <Setter.Value>
                                <ToolTip MaxWidth="400">
                                    <TreeView Grid.Row="1"  ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
              Name="treeView"
          ItemsSource="{Binding Titles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
          Style="{StaticResource MahApps.Styles.TreeView.Virtualized}">
                                        <TreeView.ItemContainerStyle>
                                            <Style TargetType="TreeViewItem" BasedOn="{StaticResource MahApps.Styles.TreeViewItem}">
                                                <Setter Property="IsExpanded" Value="True"/>
                                            </Style>
                                        </TreeView.ItemContainerStyle>
                                        <TreeView.ItemTemplate>
                                            <HierarchicalDataTemplate ItemsSource="{Binding  Specs, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" >

                                                <Grid >
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition/>
                                                        <ColumnDefinition Width="auto"/>
                                                        <ColumnDefinition Width="auto"/>
                                                    </Grid.ColumnDefinitions>

                                                    <Label Grid.Column="0" Content="Title:"/>
                                                    <TextBox Grid.Column="1" TextWrapping="Wrap" Text="{Binding Name, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                         ScrollViewer.VerticalScrollBarVisibility="Auto"  
                                         ctrl:TextBoxHelper.Watermark="Title" MaxHeight="72"
                                          Margin="1"
                                         Background="DarkCyan"/>

                                                </Grid>

                                                <HierarchicalDataTemplate.ItemTemplate>
                                                    <DataTemplate>
                                                        <Grid >
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="auto"/>
                                                                <ColumnDefinition/>
                                                                <ColumnDefinition Width="auto"/>
                                                                <ColumnDefinition  Width="auto"/>
                                                            </Grid.ColumnDefinitions>

                                                            <Label Grid.Column="0" Content="Spec:"/>


                                                            <TextBox Grid.Column="1" TextWrapping="Wrap" Text="{Binding Name, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" 
                                         ScrollViewer.VerticalScrollBarVisibility="Auto"  
                                         ctrl:TextBoxHelper.Watermark="Specification" MaxHeight="72"
                                          Margin="1"/>

                                                            <TextBox Grid.Column="2" Width="90" TextWrapping="Wrap" VerticalContentAlignment="Center" Text="{Binding Price, UpdateSourceTrigger=LostFocus,StringFormat={}{0:c} , Mode=TwoWay}" 
                                             ctrl:TextBoxHelper.Watermark="Price" 
                                              Margin="1"/>

                                                        </Grid>
                                                    </DataTemplate>
                                                </HierarchicalDataTemplate.ItemTemplate>
                                            </HierarchicalDataTemplate>
                                        </TreeView.ItemTemplate>
                                    </TreeView>
                                </ToolTip>
                            </Setter.Value>
                        </Setter>
                        <DataTrigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation Storyboard.TargetProperty="Opacity" SpeedRatio="0.5" From="0.2" To="1" Duration="0:0:1" AutoReverse="True" RepeatBehavior="Forever" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.EnterActions>
                        <DataTrigger.ExitActions>
                            <BeginStoryboard>
                                <Storyboard FillBehavior="Stop">
                                    <DoubleAnimation Storyboard.TargetProperty="Opacity" To="1" Duration="0:0:1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.ExitActions>
                    </DataTrigger>
                </Style.Triggers>
            </Style>

