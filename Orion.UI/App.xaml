            <Style
                x:Key="buttonInfoStyle"
                BasedOn="{StaticResource MahApps.Styles.Button}"
                TargetType="Button">

                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Grid>
                                <icon:PackIconFontAwesome
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Kind="InfoSolid">
                                    <icon:PackIconFontAwesome.LayoutTransform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleX="0.75" ScaleY="0.75" />
                                        </TransformGroup>
                                    </icon:PackIconFontAwesome.LayoutTransform>
                                </icon:PackIconFontAwesome>
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <!--<Setter Property="Background" Value="Transparent"/>-->
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding HasTitles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Value="True">
                                                <Setter Property="Background" Value="#32B974" />
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding HasTitles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Value="False">
                                                <Setter Property="Background" Value="Transparent" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="Command" Value="{Binding Path=DataContext.ShowItemTitlesCommand, RelativeSource={RelativeSource AncestorType={x:Type DataGrid}}}" />
                <Setter Property="CommandParameter" Value="{Binding}" />
                <Setter Property="ToolTip" Value="Item Info" />

                <Style.Triggers>
                    <DataTrigger Binding="{Binding HasTitles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Value="False">
                        <Setter Property="Background" Value="Transparent" />
                        <Setter Property="ToolTip" Value="Item Info" />
                    </DataTrigger>
                    <DataTrigger Binding="{Binding HasTitles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" Value="True">
                        <Setter Property="Background" Value="#32B974" />
                        <Setter Property="ToolTip">
                            <Setter.Value>
                                <ToolTip MaxWidth="400">
                                    <TreeView
                                        Name="treeView"
                                        Grid.Row="1"
                                        ItemsSource="{Binding Titles, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                        ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                                        Style="{StaticResource MahApps.Styles.TreeView.Virtualized}">
                                        <TreeView.ItemContainerStyle>
                                            <Style BasedOn="{StaticResource MahApps.Styles.TreeViewItem}" TargetType="TreeViewItem">
                                                <Setter Property="IsExpanded" Value="True" />
                                            </Style>
                                        </TreeView.ItemContainerStyle>
                                        <TreeView.ItemTemplate>
                                            <HierarchicalDataTemplate ItemsSource="{Binding Specs, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}">

                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="auto" />
                                                        <ColumnDefinition />
                                                        <ColumnDefinition Width="auto" />
                                                        <ColumnDefinition Width="auto" />
                                                    </Grid.ColumnDefinitions>

                                                    <Label Grid.Column="0" Content="Title:" />
                                                    <TextBox
                                                        Grid.Column="1"
                                                        MaxHeight="72"
                                                        Margin="1"
                                                        ctrl:TextBoxHelper.Watermark="Title"
                                                        Background="DarkCyan"
                                                        ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                        Text="{Binding Name, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                                        TextWrapping="Wrap" />

                                                </Grid>

                                                <HierarchicalDataTemplate.ItemTemplate>
                                                    <DataTemplate>
                                                        <Grid>
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="auto" />
                                                                <ColumnDefinition />
                                                                <ColumnDefinition Width="auto" />
                                                                <ColumnDefinition Width="auto" />
                                                            </Grid.ColumnDefinitions>

                                                            <Label Grid.Column="0" Content="Spec:" />


                                                            <TextBox
                                                                Grid.Column="1"
                                                                MaxHeight="72"
                                                                Margin="1"
                                                                ctrl:TextBoxHelper.Watermark="Specification"
                                                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                                Text="{Binding Name, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}"
                                                                TextWrapping="Wrap" />

                                                            <TextBox
                                                                Grid.Column="2"
                                                                Width="90"
                                                                Margin="1"
                                                                VerticalContentAlignment="Center"
                                                                ctrl:TextBoxHelper.Watermark="Price"
                                                                Text="{Binding Price, UpdateSourceTrigger=LostFocus, StringFormat={}{0:c}, Mode=TwoWay}"
                                                                TextWrapping="Wrap" />

                                                        </Grid>
                                                    </DataTemplate>
                                                </HierarchicalDataTemplate.ItemTemplate>
                                            </HierarchicalDataTemplate>
                                        </TreeView.ItemTemplate>
                                    </TreeView>
                                </ToolTip>
                            </Setter.Value>
                        </Setter>
                        <DataTrigger.EnterActions>
                            <BeginStoryboard>
                                <Storyboard>
                                    <DoubleAnimation
                                        AutoReverse="True"
                                        RepeatBehavior="Forever"
                                        SpeedRatio="0.5"
                                        Storyboard.TargetProperty="Opacity"
                                        From="0.2"
                                        To="1"
                                        Duration="0:0:1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.EnterActions>
                        <DataTrigger.ExitActions>
                            <BeginStoryboard>
                                <Storyboard FillBehavior="Stop">
                                    <DoubleAnimation
                                        Storyboard.TargetProperty="Opacity"
                                        To="1"
                                        Duration="0:0:1" />
                                </Storyboard>
                            </BeginStoryboard>
                        </DataTrigger.ExitActions>
                    </DataTrigger>
                </Style.Triggers>
            </Style>

